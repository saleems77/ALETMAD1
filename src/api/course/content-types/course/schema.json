{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "courseName": {
      "type": "string",
      "required": true
    },
    "requirements": {
      "type": "text"
    },
    "targetAudience": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "isFree": {
      "type": "boolean",
      "default": true
    },
    "accessInstructions": {
      "type": "string"
    },
    "hasEntryTest": {
      "type": "boolean"
    },
    "track": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::track.track",
      "inversedBy": "courses"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "participants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "entry_test": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::entry-test.entry-test",
      "mappedBy": "course"
    },
    "videos": {
      "type": "component",
      "component": "shared.videos",
      "repeatable": true
    },
    "learningObjectives": {
      "type": "text"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "reviewStatus": {
      "type": "enumeration",
      "default": "\"Pending\"",
      "enum": [
        "\"Pending\"",
        "\"Approved\"",
        "\"Rejected\""
      ]
    },
    "rejectionReason": {
      "type": "string"
    },
    "instructorShare": {
      "type": "decimal",
      "default": 70
    },
    "platformShare": {
      "type": "decimal",
      "default": 20
    },
    "marketerShare": {
      "type": "decimal",
      "default": 10
    },
    "course_chat": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course-chat.course-chat",
      "inversedBy": "course"
    },
    "coupon": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::coupon.coupon",
      "mappedBy": "course"
    },
    "invitation_link": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::invitation-link.invitation-link",
      "mappedBy": "course"
    },
    "materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::material.material",
      "mappedBy": "course"
    },
    "certificates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::certificate.certificate",
      "mappedBy": "course"
    },
    "certificate": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::certificate.certificate",
      "mappedBy": "course"
    }
  }
}
